/**********top header styling************/

.top-header{
    position: relative;
    display: flex;
    height: 200px;
    width: 100%;
    justify-content: center;
    align-items: center;
    border-bottom: solid blue 1px;
    background: linear-gradient(100deg, #0b0120,#3778c2);
}

/***********top navigation style***********/

.top-navigation{
    position: sticky;
    position: -webkit-sticky;
    top: 0;
    width: 100%;
    height: 100px;
    background: linear-gradient(100deg, #0b0120,#3778c2);
}

.top-navigation::before{
    content: '';
    position: absolute;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 100px;
    background: #d3f5f8;
    clip-path: polygon(100% 60%,0 100%,100% 100%);
}

.nav {
    height: 50px;
    width: 100%;
    position: relative;
}

.logo-container{
    display: inline;
}

.nav-logo{
    display: inline-block;
    font-size: 22px;
    color: #fff;
    padding: 10px 10px 10px 10px;
}

.nav-btn {
    display: none;
}

.nav-btn button{
    display: block;
}

.nav-links {
    display: inline;
    float: right;
    font-size: 18px;
}

.nav-links2 {
    display: inline;
    display: flex;
    font-size: 18px;
    width: 200px;
    height: calc(100vh - 50px);
    overflow-y: auto;
}

.nav-links a {
    display: inline-block;
    padding: 13px 10px 13px 10px;
    text-decoration: none;
    color: #efefef;
}

.nav-links2 a {
    display: inline-block;
    padding: 13px 10px 13px 10px;
    text-decoration: none;
    color: #efefef;
    display: flex;
    width: 200px;
}

.nav .nav-links2 {
    position: absolute;
    display: none;
    width: 15%;
    background: linear-gradient(50deg, #0b0120,#3778c2);;
    transition: all 0.3s ease-in;
    overflow-y: hidden;
    top: 50px;
    right: 0;
}

.profile{
    width: 40px;
    height: 35px;
    border: 0;
    border-radius: 20px;
}

.nav-links a:hover {
  
}

@media screen and (max-width:600px) {
    .nav-btn {
        display: inline-block;
        position: absolute;
        height: 40px;
        right: 0px;
        top: 0px;
    }
    .nav-btn button {
        display: inline-block;
        width: 50px;
        height: 40px;
        margin: 5px;
    }
    
    .nav .nav-links {
        position: absolute;
        display: block;
        width: 50%;
        background: linear-gradient(70deg, #0b0120,#3778c2);;
        transition: all 0.3s ease-in;
        overflow-y: hidden;
        top: 50px;
        right: 0;
    }

    .nav .nav-links > a {
        display: block;
        width: 100%;
    }

    .nav .nav-links {
        height: calc(100vh - 50px);
        overflow-y: auto;
    }

    .nav .nav-links2{
        display: none;
    }

    .nav-links2 a {
        display: none;
    }
    
    .profile{
        display: none;
    }
}








